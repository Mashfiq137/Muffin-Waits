{
    "manifest_version": 2,
    "name": "Muffin Waits",
    "version": "0.0.1",
    "description": "A NSU student can join a class in Google Meet only after the faculty joins. This muffin notifies you when the faculty arrives",
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/",
        "storage",
        "notifications"
    ],
    "icons": {
        "128": "muffin128.png",
        "48": "muffin48.png",
        "16": "muffin16.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}